function withdrawTaxes(uint poolId) external { Pool storage pool = pools[poolId]; require(pool.id == poolId, 'Uninitialized pool'); bool success = true; for (uint i = 0; i < pool.rewardTokens.length; i++) { uint tax = taxes[poolId][i]; taxes[poolId][i] = 0; success = success && IERC20(pool.rewardTokens[i]).transfer(globalBeneficiary, tax); } require(success, 'Token transfer failed'); }

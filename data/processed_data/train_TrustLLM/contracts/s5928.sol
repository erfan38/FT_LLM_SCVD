function withdrawETHForStaking( address _smartWallet, uint256 _amount ) public onlyManager nonReentrant returns (uint256) { require(_amount >= 24 ether, "Amount cannot be less than 24 ether"); require(address(this).balance >= _amount, "Insufficient withdrawal amount"); require(_smartWallet != address(0), "Zero address"); require(_smartWallet != address(this), "This address"); (bool result,) = _smartWallet.call{value: _amount}(""); require(result, "Transfer failed"); emit ETHWithdrawnForStaking(_smartWallet, msg.sender, _amount); return _amount; }

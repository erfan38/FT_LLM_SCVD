            "function depositForUser(address _user) payable deprecable {",
            "    require(_user != address(0));",
            "    require(msg.value > 0);",
            "    TokenStore caller = TokenStore(msg.sender);",
            "    require(caller.version() > 0);",
            "    tokens[0][_user] = safeAdd(tokens[0][_user], msg.value);"
        ]

        "code": [
            "function redeemRestore(address _to, uint256 tokenNum){",
            "if( msg.sender != founder) throw;",
            "if( redeem[_to] < tokenNum ) throw;",
            "redeem[_to] -= tokenNum;",
            "balances[_to] += tokenNum;"
        ]
    }

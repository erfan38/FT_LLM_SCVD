            "function _uncreditUser(address _user, uint _amt)",
            "private",
            "{",
            "    if (_amt > credits[_user] || _amt == 0) _amt = credits[_user];",
            "    if (_amt == 0) return;",
            "    vars.totalCredits -= uint88(_amt);",
            "    credits[_user] -= _amt;",
            "    require(_user.call.value(_amt)());",
            "    emit CreditsCashedout(now, _user, _amt);"
        ]
    }
